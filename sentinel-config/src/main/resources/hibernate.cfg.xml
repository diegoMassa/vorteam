        <mapping resource="com/vortexbird/sentinel/modelo/SegAuditoria.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegRol.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegCambioPass.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegCompania.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegSistemaCia.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegParametro.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegSistema.hbm.xml" />
        <mapping resource="com/vortexbird/sentinel/modelo/SegPermiso.hbm.xml" />
